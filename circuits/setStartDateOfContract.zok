from "ecc/babyjubjubParams" import main as curveParams
from "ecc/edwardsScalarMult" import main as scalarMult
from "ecc/edwardsCompress" import main as edwardsCompress
from "utils/pack/bool/pack256.zok" import main as bool_256_to_field
from "utils/pack/bool/nonStrictUnpack256.zok" import main as field_to_bool_256
from "hashes/poseidon/poseidon.zok" import main as poseidon
from "./common/merkle-tree/mimc/altbn254/verify-membership/height32.zok" import main as checkRoot

def main(\
	private field startDateOfContractParam,\
	private field startDateOfContract_oldCommitment_owner_secretKey,\
	public field startDateOfContract_oldCommitment_nullifier,\
	private  field startDateOfContract_oldCommitment_value,\
	private field startDateOfContract_oldCommitment_salt,\
	private bool startDateOfContract_oldCommitment_isDummy,\
	public field commitmentRoot,\
	private field startDateOfContract_oldCommitment_membershipWitness_index,\
	private field[32] startDateOfContract_oldCommitment_membershipWitness_siblingPath,\
	private field startDateOfContract_newCommitment_owner_publicKey,\
	private field startDateOfContract_newCommitment_salt,\
	public field startDateOfContract_newCommitment_commitment\
) -> (bool):

        
        // We need to hard-code each stateVarId into the circuit:
        field startDateOfContract_stateVarId_field = 13


        field startDateOfContract = startDateOfContract_oldCommitment_value

startDateOfContract = startDateOfContractParam


        // startDateOfContract_oldCommitment - PoKoSK:
        // The correctness of this secret key will be constrained within the oldCommitment existence check.

        field[2] startDateOfContract_oldCommitment_owner_publicKey_point = scalarMult(field_to_bool_256(startDateOfContract_oldCommitment_owner_secretKey), [curveParams().Gu, curveParams().Gv], curveParams())

        bool startDateOfContract_oldCommitment_owner_publicKey_sign = edwardsCompress(startDateOfContract_oldCommitment_owner_publicKey_point)[0]

        bool[254] startDateOfContract_oldCommitment_yBits = field_to_bool_256(startDateOfContract_oldCommitment_owner_publicKey_point[1])[2..256]
        startDateOfContract_oldCommitment_yBits[0] = startDateOfContract_oldCommitment_owner_publicKey_sign

        field startDateOfContract_oldCommitment_owner_publicKey = bool_256_to_field([false, false, ...startDateOfContract_oldCommitment_yBits])


        // Nullify startDateOfContract:

        field startDateOfContract_oldCommitment_nullifier_check_field = poseidon([\
          startDateOfContract_stateVarId_field,\
          startDateOfContract_oldCommitment_owner_secretKey,\
          startDateOfContract_oldCommitment_salt\
        ])

        assert(\
        field_to_bool_256(startDateOfContract_oldCommitment_nullifier)[8..256] == field_to_bool_256(startDateOfContract_oldCommitment_nullifier_check_field)[8..256]\
        )
    
        


        // startDateOfContract_oldCommitment_commitment: preimage check


        field startDateOfContract_oldCommitment_commitment_field = poseidon([\
          startDateOfContract_stateVarId_field,\
          startDateOfContract_oldCommitment_value,\
          startDateOfContract_oldCommitment_owner_publicKey,\
          startDateOfContract_oldCommitment_salt        ])


        // startDateOfContract_oldCommitment_commitment: existence check


        field startDateOfContract_commitmentRoot_check = checkRoot(\
          startDateOfContract_oldCommitment_membershipWitness_siblingPath,\
          startDateOfContract_oldCommitment_commitment_field,\
          startDateOfContract_oldCommitment_membershipWitness_index\
        )


        // Note: Don't bother actually asserting existence, if the oldCommitment is a dummy:
        startDateOfContract_commitmentRoot_check = if startDateOfContract_oldCommitment_isDummy == true then commitmentRoot else startDateOfContract_commitmentRoot_check fi


        assert(\
          field_to_bool_256(commitmentRoot)[8..256] == field_to_bool_256(startDateOfContract_commitmentRoot_check)[8..256]\
        )


        // prepare secret state 'startDateOfContract' for commitment

        field startDateOfContract_newCommitment_value_field = startDateOfContract

        // startDateOfContract_newCommitment_commitment - preimage check

        field startDateOfContract_newCommitment_commitment_check_field = poseidon([\
          startDateOfContract_stateVarId_field,\
          startDateOfContract_newCommitment_value_field,\
          startDateOfContract_newCommitment_owner_publicKey,\
          startDateOfContract_newCommitment_salt\
        ])

        assert(\
          field_to_bool_256(startDateOfContract_newCommitment_commitment)[8..256] == field_to_bool_256(startDateOfContract_newCommitment_commitment_check_field)[8..256]\
        )

         return true